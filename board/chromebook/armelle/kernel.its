/dts-v1/;

/ {
	description = "Chrome OS kernel image with FDT";

	images {
		kernel-1 {
			description = "kernel";
			//data = /incbin/("Image");
			data = /incbin/("../../../Image.prawn");
			type = "kernel_noload";
			arch = "arm64";
			os = "linux";
			compression = "none";
			load = <0>;
			entry = <0>;
		};

		fdt-1 {
			description = "rk3399-gru-kevin.dtb";
			//data = /incbin/("rk3399-gru-kevin.dtb");
			data = /incbin/("../../../rk3399-gru-kevin.dtb.prawn");
			type = "flat_dt";
			arch = "arm64";
			compression = "none";

			hash-1 {
				algo = "sha1";
			};
		};
	};

	configurations {
		default = "conf-1";
		conf-1 {
			kernel = "kernel-1";
			fdt = "fdt-1";
		};
	};
};
